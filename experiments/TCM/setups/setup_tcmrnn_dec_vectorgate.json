{
    "model": {
        "class": "TCMRNN",
        "hidden_dim": 21,
        "tau": 10,
        "mem_gate_type": "vector",
        "output_type": "decision"
    },
    "supervised_env": {
        "class": "FreeRecall",
        "vocabulary_num": 20,
        "memory_num": 5,
        "retrieve_time_limit": 5,
        "start_recall_cue": false,
        "reset_state_before_test": false,
        "batch_size": 16
    },
    "supervised_training": {
        "num_iter": 200000,
        "test": false,
        "test_iter": 500,
        "save_iter": 2000,
        "stop_test_accu": 0.99,
        "use_memory": true,
        "criterion": {
            "class": "FreeRecallSumMSELoss",
            "var_weight": 3.0
        },
        "optimizer": {
            "class": "Adam",
            "lr": 1e-3,
            "lr_decay_factor": 0.5,
            "lr_decay_patience": 10,
            "lr_decay_threshold": 1e-3,
            "min_lr": 1e-8
        }
    },
    "load_saved_model": false,
    "pretrain": true,
    "context_num": 20
}